  "license": "ISC",
  "devDependencies": {
    "@headlessui/react": "^1.6.1",
    "@heroicons/react": "^1.0.6",
    "@tailwindcss/forms": "^0.5.1",
    "autoprefixer": "^10.4.7",
    "buffer": "^6.0.3",
    "cssnano": "^5.1.7",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "parcel": "^2.5.0",
    "postcss": "^8.4.13",
    "postcss-import": "^14.1.0",
    "prettier": "^2.6.2",
    "process": "^0.11.10",
    "tailwindcss": "^3.0.24"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "gh-pages": "^4.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
